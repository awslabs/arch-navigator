{
  "name": "@arch-navigator",
  "version": "0.0.0-alpha",
  "private": true,
  "packageManager": "pnpm@10.21.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/arch-navigator.git"
  },
  "homepage": "https://github.com/awslabs/arch-navigator",
  "bugs": {
    "url": "https://github.com/awslabs/arch-navigator/issues"
  },
  "scripts": {
    "clean": "turbo run clean; rm -rf .turbo apps/*/.turbo packages/*/.turbo node_modules build",
    "clean:web": "turbo run clean --filter=@arch-navigator/web",
    "clean:electron": "turbo run clean --filter=@arch-navigator/electron",
    "clean:ui": "turbo run clean --filter=@arch-navigator/ui",
    "dev:web": "turbo run dev --filter=@arch-navigator/web",
    "dev:api": "turbo run dev --filter=@arch-navigator/api",
    "dev:electron": "turbo run dev --filter=@arch-navigator/electron",
    "dev:storybook": "turbo run storybook --filter=@arch-navigator/ui",
    "build:web": "turbo run build --filter=@arch-navigator/web",
    "build:electron": "turbo run build --filter=@arch-navigator/electron",
    "build:storybook": "turbo run build-storybook --filter=@arch-navigator/ui",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/node": "^24.10.0",
    "turbo": "^2.6.1",
    "vite": "^7.2.2"
  }
}
